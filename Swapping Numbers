#include<stdio.h>
void swapr(int *x, int *y);
void main()
{
    int a , b;
    printf("Enter a: ");
    scanf("%d",&a);
    printf("Enter b: ");
    scanf("%d",&b);
    printf("\n a = %d b= %d", a,b);
    swapr(&a,&b);
    printf("\n a = %d b= %d", a,b);
}
void swapr (int *x, int *y)
{
    int t;
    t = *x;
    *x = *y;
    *y = t;  
}
